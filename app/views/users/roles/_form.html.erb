<%= simple_form_for(@users_role, wrapper: :form) do |f| %>
  <div uk-grid>
    <div class="uk-width-auto">
      <%= yield :title %>
    </div>
    <div class="uk-width-expand">
      <div class="uk-float-right">
        <%= yield :links %>
        <%= f.button :submit, class: 'uk-button uk-button-primary' %>
      </div>
    </div>
  </div>

  <%= f.error_notification %>

  <div>
    <%= f.input :name %>
  </div>
<% end %>
